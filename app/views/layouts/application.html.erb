<!DOCTYPE html>
<html>
  <head>
    <title>FaceBork</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/javascript">
      window.splashimage = "<%= image_url('/babe/emma_splash.jpeg') %>"
      window.fb1 = "<%= image_url('/fb_icons/fb1.png') %>"
      window.fb2 = "<%= image_url('/fb_icons/fb2.png') %>"
      window.fb3 = "<%= image_url('/fb_icons/fb3.png') %>"
    </script>


    <%= yield %>
  </body>
</html>
